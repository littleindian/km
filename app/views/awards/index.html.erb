<h1>Listing awards</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Awardgivername</th>
      <th>Awardlink</th>
      <th>Accepted</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @awards.each do |award| %>
      <tr>
        <td><%= award.userid %></td>
        <td><%= award.awardGiverName %></td>
        <td><%= award.awardLink %></td>
        <td><%= award.accepted %></td>
        <td><%= link_to 'Show', award %></td>
        <td><%= link_to 'Edit', edit_award_path(award) %></td>
        <td><%= link_to 'Destroy', award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Award', new_award_path %>
